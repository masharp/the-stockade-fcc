{
  "name": "the-stockade",
  "version": "0.0.1",
  "private": true,
  "homepage": "http://the-stockade.herokuapp.com",
  "bugs": {
    "url": "https://github.com/masharp/the-stockade-fcc/issues"
  },
  "author": "Michael Sharp (http://softwareontheshore.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/masharp/the-stockade-fcc"
  },
  "scripts": {
    "sass": "sass --watch ./public/stylesheets/sass/style.scss:./public/stylesheets/style.css",
    "scan": "eslint ./public/javascripts/global.js",
    "bundle": "babel ./public/javascripts/global.js -o ./public/javascripts/compiled.js && browserify ./public/javascripts/compiled.js -o ./public/javascripts/bundle.js",
    "start": "node ./bin/www",
    "test": "jasmine JASMINE_CONFIG_PATH=test/jasmine.json"
  },
  "engines": {
    "node": "~5.8.0",
    "npm": "~3.5.3"
  },
  "eslintConfig": {
    "rules": {
      "strict": "off"
    }
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "browserify": "^13.0.0",
    "compression": "latest",
    "debug": "~2.2.0",
    "dotenv": "~2.0.0",
    "express": "~4.13.1",
    "express-session": "^1.13.0",
    "jade": "~1.11.0",
    "mongodb": "~2.1.9",
    "morgan": "~1.6.1",
    "parseurl": "~1.3.0",
    "react": "~0.14.7",
    "react-d3": "~0.4.0",
    "react-dom": "~0.14.7",
    "request": "^2.69.0",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.4.5",
    "socket.io-client": "^1.4.5"
  },
  "devDependencies": {
    "babel-cli": "~6.6.4",
    "eslint": "~2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "latest",
    "expect": "~1.14.0",
    "jasmine": "~2.4.1"
  }
}
